<%= form_for(@scheduled_group) do |r| %>
 <div id="active_admin_content"  class="without_sidebar">
    <div id="main_content_wrapper">
      <div id="main_content">
          <div class="panel">
            <div class="panel_contents">
            <div class="attributes_table liaison" id="attributes_table_liaison_3">
              <%= render 'layouts/scheduled_group_table' %>
              <h3>To complete the scheduling of this group, press Confirm below. Otherwise, Cancel or return to the previous screen.</h3>
              <div class="actions">
                <%= submit_tag 'Confirm', { :confirm => "Are you sure you want to complete this transaction?", :regid => @scheduled_group.registration_id } %>
                <%= submit_tag "Cancel", { :confirm => "Are you sure you want to cancel and return to the Requests page?" } %>
              </div>
 <% end %>
            </div>
            </div>
          </div>
         </div>
        </div>
     </div>
   </div>

